sub ensure-authors-correct-in-toml($file) {

    say "ensuring authors correct in toml for $file";

    my %toml = from-toml($file.IO.slurp);
    %toml<package><authors>               = [ "klebs <none>" ];
    %toml<package><description>           = "todo";
    %toml<package><license>               = "MIT";

    #why does this come in with quotes on the key? 
    #
    #(fixed with a manual step after generation)
    #
    #%toml<package><metadata.cpp-authors> = @cpp-authors;

    $file.IO.spurt: to-toml(%toml);
}

race for @tomls {
    ensure-authors-correct-in-toml($_);
}
